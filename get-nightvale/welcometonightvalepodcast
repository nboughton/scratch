    <item>
      <guid isPermaLink="false">cd4fdacefe37b159de51bc5209d7e938</guid>
      <title>24 - The Mayor</title>
      <pubDate>Sat, 01 Jun 2013 12:00:00 -0000</pubDate>
      <link>https://beta.prx.org/stories/229729</link>
      <description>
